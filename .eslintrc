{
  "extends": "airbnb",
  "rules": {
    "func-names": ["error", "never"],
    "prefer-arrow-callback": 0,
    "consistent-return": 0,
    "no-var": 0,
    "comma-dangle": 0,
    "radix": 0,
    "spaced-comment": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true
  }
}